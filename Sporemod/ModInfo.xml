<mod displayName="Spore-LuaAPI" unique="luaapi" description="Enables loading lua mods to modify the game." isExperimental="$SPOREMOD_EXPERIMENTAL" installerSystemVersion="1.0.1.2" dllsBuild="$SPOREMOD_DLLS_BUILD">
	<prerequisite>1.SporeLuaAPI.dll</prerequisite>
	<prerequisite>1.SporeLuaAPI.pdb</prerequisite>
	<prerequisite>1.SporeLuaAPI.lib</prerequisite>
	<prerequisite game="Spore">LuaSpore.package</prerequisite>

	<remove>1.SporeLuaAPI.dll</remove>
	<remove>1.SporeLuaAPI.pdb</remove>
	<remove>1.SporeLuaAPI.lib</remove>
	<remove game="Spore">LuaSpore.package</remove>
	<!--Remove stuff from old versions of this mod-->
</mod>